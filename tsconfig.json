{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"outDir": "dist",
		"ignoreDeprecations": "5.0",
		"verbatimModuleSyntax": true,
		"types": [
			"vitest/globals",
			"@sveltejs/kit",
			"@sveltejs/kit/vite"
		]
	},
	"exclude": [
		"dist"
	]
}
